import gameEngine.Math;

public class Test {

	public static void main() {
		
		Matrix4f matrix;
		Matrix4f n;
		
		
		matrix=Math.getMatrix(new Vector2f(0,0), 0, 0);
		n=Math.getMatrix(new Vector2f(0,0), 0, 0);
		if(n==matrix) {
			System.out.println("yes");
		}
		
		
		
	}
	
	
	
	
	
	
	
}
