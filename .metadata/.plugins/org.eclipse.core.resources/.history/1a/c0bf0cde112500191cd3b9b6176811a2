package gameEngine;



public class Tilepositions{

private float x,y,width,height;
private Tiles tile;


    public Tilepositions(float x,float y,float width,float height, Tiles tile){
            this.tile=tile;
            this.x=x;
            this.y=y;
            this.width=width;
            this.heigth=height;
    }

     public float getX(){
        return x; }       

     public float getY(){
            return y; }  

     public float getWidth(){
                return width; }
     public float getHeight(){
         return height;
     }            
                
    public Tiles getTile(){
                return tile; }  




}